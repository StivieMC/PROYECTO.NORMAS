    ViewData["Title"] = "Delete";
}
<font color="Red" face="Book antiqua,arial">
    <h1>Eliminar</h1>
</font>
    <h3>Esta seguro que desea eliminar esta norma?</h3>
    <div>
        <h4>-------------------------------------------------------</h4>
        <hr />


        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Clave)
